<div>
    <h1 class="card z-depth-3 center-align">Read the Latest from 'The NY-Time' in Tech</h1>
    <div class="row">
      <div class="col s3"></div>
      <h5 class="card z-depth-3 center-align col s6">Click 'Scrape' for the fresh articles</h5>
    </div>
</div>

<div class="row">
  <div class="col s2"></div>
  <div class ="col s8 left-align">
    {{#each article}}
      <div class="card-panel">
        <h4>{{this.title}}</h4>
        {{!-- goes to article.handlebars --}}
        <a href = "/readArticle/{{this._id}}" class="waves-effect waves-light btn pink lighten-1" id="btnSignUp">Read Articles</a>
      </div>
    {{/each}}
  </div>
</div>